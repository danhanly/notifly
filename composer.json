{
  "name": "panday/panday",
  "description": "A notifications package to provide UI notifications to users and admins alike",
  "type": "library",
  "license": "GPL-2.0",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Dan Hanly",
      "email": "danhanly@gmail.com"
    },
    {
      "name": "Mateusz Tracz",
      "email": "matiqing@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "symfony/config": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Panday\\": "src/Panday"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Panday\\Tests\\": "tests/utils/classes"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}